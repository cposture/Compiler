PROGRAM EX01;
VAR A,B,C;
BEGIN
	FOR C:=1 STEP 1 UNTIL 2 DO
	BEGIN
		WRITE(C);
		
		READ(A);
		READ(B);
		
		/*
		aB
		*/
		IF A<>B THEN
			WRITE(A);

		IF A||B THEN
			WRITE(1);
		ELSE
			WRITE(0);
		
		IF !A THEN
			WRITE(0);
		ELSE
			WRITE(1);
		
		IF A&B THEN
			WRITE(1);
		ELSE
			WRITE(0);
		
		A*=2;
		WRITE(A);
		A/=2;
		WRITE(A);
	END;
END.


